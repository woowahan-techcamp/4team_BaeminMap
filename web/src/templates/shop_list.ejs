<% _.each(data, (shop) => {
    const shopLogoImageUrl = shop.shopLogoImageUrl.replace('http://woowahan1.vipweb.kr', 'https://baeminmap3.testi.kr')
    const coordinates = `${shop.location.latitude}_${shop.location.longitude}`
%>
<div class="shop" id="<%= shop.shopNumber %>" data-shopnumber="<%= shop.shopNumber %>" data-coordinates="<%= coordinates %>">
    <a class="shop-layer" data-shopnumber="<%= shop.shopNumber %>" data-categoryenglishname="<%= shop.categoryEnglishName%>"></a>
    <div class="shop-image-wrapper">
        <img src="<%= shopLogoImageUrl %>" onerror="this.src='../static/default.png'" alt="">
    </div>
    <div class="shop-abstract-wrapper">
        <span class="shop-distance">
            <% if(shop.distance < 1) {%>
            <%= shop.distance.toFixed(3)*1000 + "m" %><% } else {%>
            <%= shop.distance.toFixed(1) + "km" %><% } %>
        </span>
        <h2><%= shop.shopName %></h2>
        <span class="shop-rating">
            <span class="img-star star-rate">
                <span class="img-star inner-star-rate" style="width: <%= (shop.starPointAverage / 5) * 100 %>%"></span>
            </span>
        </span>
        <p class="shop-address"><%= shop.address + ' ' + shop.addressDetail %></p>
        <p class="shop-review">
            <span>리뷰 <span><%= shop.reviewCount %></span></span>
            <span>사장님댓글 <span><%= shop.reviewCountCeo %></span></span>
        </p>
    </div>
</div>
<% }); %>